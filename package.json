{
  "name": "library-app",
  "version": "4.17.0",
  "description": "Demo application for learning Ember.js v3 (Octane)",
  "repository": {
    "type": "git",
    "url": "https://github.com/zoltan-nz/library-app"
  },
  "license": "MIT",
  "author": "Zoltan Debre",
  "private": false,
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "run-p --aggregate-output -c -l \"lint:!(fix)\"",
    "lint:fix": "run-p --aggregate-output -c -l \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "run-p --aggregate-output -l lint test:*",
    "test:ember": "ember test",
    "test:coverage": "COVERAGE=true ember test",
    "format": "prettier --cache --list-different --write --ignore-unknown '**'",
    "format:check": "prettier --cache --list-different --ignore-unknown '**'",
    "ci": "run-s -l lint:js format:check test:coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@fortawesome/ember-fontawesome": "^0.4.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.2.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.2",
    "ember-bootstrap": "^5.1.1",
    "ember-classic-decorator": "^3.0.0",
    "ember-cli": "^4.7.0",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-code-coverage": "^1.0.3",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-update": "^1.0.1",
    "ember-concurrency": "^2.3.7",
    "ember-data": "^4.6.3",
    "ember-export-application-global": "^2.0.1",
    "ember-faker": "^2.0.1",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-local-storage": "^2.0.4",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-sinon": "^5.0.0",
    "ember-source": "^4.7.0",
    "ember-template-lint": "^4.14.0",
    "ember-welcome-page": "^6.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "husky": "^8.0.1",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "sass": "^1.54.9",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "14.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "husky": {
    "hooks": {
      "pre-push": "run-p lint:js:check format:check test:ember"
    }
  }
}
